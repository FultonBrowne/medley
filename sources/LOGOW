                                   (BITMAPHEIGHT LOGOBITMAP)
                                   TITLE-BOTTOM-HEIGHT))
                           (TEMP (BITMAPCREATE WLEN WHT))
                           (IMAGE (BITMAPCREATE WLEN WHT))
                           (MASK (BITMAPCREATE WLEN WHT))
